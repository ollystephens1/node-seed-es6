{
  "name": "ng-tips-api",
  "version": "1.0.0",
  "description": "Express API for ng-tips",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --config ./config/webpack.config.js --watch",
    "watch:server": "nodemon ./dist/server.bundle.js --watch ./dist",
    "eslint": "./node_modules/.bin/eslint ./src",
    "precommit": "npm run eslint"
  },
  "author": "Oliver Stephens",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.18.2",
    "husky": "^0.14.3",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.11",
    "webpack-node-externals": "^1.6.0"
  }
}
